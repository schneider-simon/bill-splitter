{
  "extends": [
    "tslint:recommended",
    "tslint-react",
    "tslint-config-prettier"
  ],
  "linterOptions": {
    "exclude": [
      "config/**/*.js",
      "node_modules/**/*.ts"
    ]
  },
  "rules": {
    "ordered-imports": false,
    "object-literal-sort-keys": false,
    "interface-name": [
      true,
      "never-prefix"
    ],
    "no-empty-interface": false,
    "no-console": false,
    "no-unused-expression": false,
    "no-unused-variable": false,
    "no-unused-parameters": false,
    "no-unused-locals": false
  }
}
